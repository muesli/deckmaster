[[keys]]
  index = 0
  [keys.widget]
    id = "pulseAudioControl"
    interval = 1000
    [keys.widget.config]
      appName = "Firefox"
      mode = "mute"
      showTitle = false

[[keys]]
  index = 1
  [keys.widget]
    id = "pulseAudioControl"
    interval = 1000
    [keys.widget.config]
      appName = "Rhythmbox"
      mode = "mute"
      showTitle = true
      
